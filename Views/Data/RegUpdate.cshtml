
@{
    ViewBag.Title = "RegUpdate";
}

<h2>RegUpdate</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    @TempData["s"]
<div class="box-body">
    <div class="form-group">
       <div class="">
            <input type="hidden" name="uid" value="@ViewBag.uid" />
        </div>
    </div>
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    <div class="form-group">
        @Html.Label("Surname", htmlAttributes: new { @class = "control-label" })
        <div class="">
            @Html.Editor("sname", new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
    <div class="form-group">
        @Html.Label("first name", htmlAttributes: new { @class = "control-label" })
        <div class="">
            @Html.Editor("fname", new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>
    <div class="form-group">
        @Html.Label("other name", htmlAttributes: new { @class = "control-label" })
        <div class="">
            @Html.Editor("othname", new { htmlAttributes = new { @class = "form-control" } })
        </div>
    </div>




    <div class="box-footer">
        <button type="submit" class="btn btn-primary">Save</button>
        <a href="javascript: history.back()" class="btn btn-default">Cancel</a>
    </div>


</div>
}

