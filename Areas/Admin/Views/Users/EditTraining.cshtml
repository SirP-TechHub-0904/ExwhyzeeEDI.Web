<style>
    * {
        box-sizing: border-box;
    }

    .one .ng-invalid::before, .five .ng-invalid::before, .seven .ng-invalid::before {
        background: red;
    }

    .one .ng-valid::before, .five .ng-valid::before, .seven .ng-valid::before {
        background: green;
    }

    .two .ng-invalid {
        border-left: solid 4px red;
    }

    .two .ng-valid {
        border-left: solid 4px green;
    }

    input, select {
        padding: 5px 5px 5px 10px;
        outline: none;
    }

    label {
        display: block;
    }

    .one span, .five span, .seven span {
        position: relative;
        display: inline-block;
    }

        .one span::before, .five span::before, .seven span::before {
            position: relative;
            content: '';
            display: block;
            width: 4px;
            left: 0;
            top: 0;
            bottom: 0;
        }

    .two input {
        border-top: none;
        border-bottom: none;
        border-right: solid 1px #aaa;
        background: linear-gradient(to bottom, #777 0%, #fff 5%, #fff 95%, #777 100% );
    }

    .three label {
        position: relative;
    }

        .three label::before {
            display: block;
            content: "";
            position: absolute;
            background: red;
            width: 4px;
            left: 0;
            height: 29px;
            top: 18px;
        }

    .three .ng-valid::before {
        background: green;
    }

    .four span, .six span, .eight span {
        position: relative;
        display: block;
    }

        .four span::after, .six span::after, .eight span::after {
            position: absolute;
            display: block;
            content: '';
            height: 29px;
            width: 4px;
            background: red;
            left: 0;
            bottom: 0;
        }

        .six span::after {
            height: 27px;
        }

    .four .ng-valid ~ span::after, .six .ng-valid ~ span::after, .eight .ng-valid ~ span::after {
        background: green;
    }

    select {
        appearance: none;
        padding-right: 14px;
    }

    .five span::after {
        display: block;
        position: absolute;
        top: 8px;
        right: 8px;
        border: solid 2px #aaa;
        content: '';
        height: 5px;
        width: 5px;
        transform: rotate(45deg);
        border-top-width: 0;
        border-left-width: 0;
        cursor: pointer;
    }

    .seven input::-webkit-calendar-picker-indicator, .eight input::-webkit-calendar-picker-indicator {
        display: none;
    }

    .five select, .six select {
        background: transparent no-repeat right center url(https://image.freepik.com/free-icon/thin-down-arrow_318-11165.jpg);
        background-size: contain;
    }

        .five select:focus, .six select:focus {
            background-image: url(http://freeiconbox.com/icon/256/25629.png);
        }
</style>
